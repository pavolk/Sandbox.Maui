<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="dynamicresources.app.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:dynamicresources.app"
    xmlns:views="clr-namespace:dynamicresources.app.Views"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
      Title="DynamicResourcePage"
      ContentTemplate="{DataTemplate views:DynamicResourcePage}"
      Route="DynamicResourcePage" />
  
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
  



</Shell>
