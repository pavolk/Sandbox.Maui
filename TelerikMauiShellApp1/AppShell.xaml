<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:local="clr-namespace:TelerikMauiShellApp1"
       x:Class="TelerikMauiShellApp1.AppShell"
       Title="AppShell">



    <ShellContent
        Title="TelerikListViewPage"
        ContentTemplate="{DataTemplate local:TelerikListViewPage}"
        Route="TelerikListViewPage" />




    

    <ShellContent
        Title="TelerikDataFormPage"
        ContentTemplate="{DataTemplate local:TelerikDataFormPage}"
        Route="TelerikDataFormPage" />






    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />



    

</Shell>