<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TaskLoaderViewTestApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TaskLoaderViewTestApp"
    Shell.FlyoutBehavior="{OnPlatform WinUI=Locked, Default=Flyout}">

    
    
    <ShellContent
        Title="BaseAsyncPage"
        ContentTemplate="{DataTemplate local:BaseAsyncPage}"
        Route="BaseAsyncPage" />
    
    <ShellContent
        Title="MasterPage"
        ContentTemplate="{DataTemplate local:MasterPage}"
        Route="MasterPage" />
    

    
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

</Shell>
